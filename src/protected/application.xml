<?xml version="1.0" encoding="utf-8"?>
<application id="SEPROSO" Mode="Debug">

  <paths>
	<using namespace="System.Data.*" />
	<using namespace="System.Security.*" />
	<using namespace="Application.code.*" />
	<using namespace="Application.code.DAO.*" />
	<using namespace="Application.data.*" />
	<using namespace="Application.controls.*" />
  </paths>
  <modules>

<!--
	<module id="cache" class="System.Caching.TMemCache" />
-->
	<module id="daos" class="DaoManager" EnableCache="true"
  		configFile="Application.data.mysql4-sqlmap">
		<dao id="UserDao" class="Application.code.DAO.UserDao" />
		<dao id="ProjectDao" class="Application.code.DAO.ProjectDao" />
		<dao id="TimeEntryDao" class="Application.code.DAO.TimeEntryDao" />
		<dao id="CategoryDao" class="Application.code.DAO.CategoryDao" />
		<dao id="ReportDao" class="Application.code.DAO.ReportsDao" />
  	</module>

	<module id="globalization"
		class="System.I18N.TGlobalization"
		Charset="UTF-8" />

  </modules>
  <services>
    <service id="page" class="TPageService" DefaultPage="User.UserList"/>
  </services>
 </application>
